<div class="row">
  <div class="col s12 m8">
    <h3><%= diary.title %></h3>
    <div class="card story">
      <div class="card-content">
        <span class="date"><%= diary. createdAt %></span>
        <%= diary.body %>
      </div>
    </div>
  </div>
  <div class="col s12 m4">
    <div class="card center-align">
      <div class="card-content">
        <span class="card-title"><%= diary.user.firstName %></span>
        <img
          src="<%= diary.user.image %>"
          class="circle responsive-img img-small"
        />
      </div>
      <div class="card-action">
        <a href="/diaries/user/<%= diary.user._id %>"
          >More From <%= diary.user.firstName %></a
        >
      </div>
    </div>
  </div>
</div>
